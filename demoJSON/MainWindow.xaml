<Window x:Class="demoJSON.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demoJSON"
        mc:Ignorable="d"
        Title="JSON Demo" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnGetJSON" Content="Hae Json dataa" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="497" Click="btnGetJSON_Click"/>
        <TextBox x:Name="txtJSON" HorizontalAlignment="Left" Height="53" Margin="10,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="497"/>
        <DataGrid x:Name="dgData" ItemsSource="{Binding}" Margin="10,95,10,10"></DataGrid>
    </Grid>
</Window>
