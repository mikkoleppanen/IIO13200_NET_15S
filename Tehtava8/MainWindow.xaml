<Window x:Class="Tehtava8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tehtava8"
        mc:Ignorable="d"
        Title="Anna Palautetta" Height="500" Width="800">
    <Grid>
        <TabControl x:Name="tabControl" Margin="10,10,0,0"  VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5">
                    <Label x:Name="label" Content="Päivämäärä:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="98,10,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label2" Content="Olen oppinut:" HorizontalAlignment="Left" Margin="10,192,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label2_Copy1" Content="Haluan oppia:" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label2_Copy4" Content="Nimi:" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="98,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="229"/>
                    <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="120" Margin="98,69,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="554"/>
                    <TextBox x:Name="textBox_Copy1" HorizontalAlignment="Left" Height="120" Margin="98,194,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="554"/>
                    <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="532,378,0,0" VerticalAlignment="Top" Width="120" Height="30"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5">
                    <Label x:Name="label2_Copy" Content="Hyvää:" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label2_Copy2" Content="Parannettavaa:" HorizontalAlignment="Left" Margin="10,133,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label2_Copy3" Content="Muuta:" HorizontalAlignment="Left" Margin="10,258,0,0" VerticalAlignment="Top"/>
                    <TextBox x:Name="textBox_Copy2" HorizontalAlignment="Left" Height="120" Margin="98,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="554"/>
                    <TextBox x:Name="textBox_Copy3" HorizontalAlignment="Left" Height="120" Margin="98,135,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="554"/>
                    <TextBox x:Name="textBox_Copy4" HorizontalAlignment="Left" Height="111" Margin="98,260,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="554"/>
                    <Button x:Name="button1" Content="Button" HorizontalAlignment="Left" Margin="532,378,0,0" VerticalAlignment="Top" Width="120" Height="30"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="57">
                <Grid Background="#FFE5E5E5">
                    <DataGrid x:Name="dgFeedback" HorizontalAlignment="Left"  Margin="10,10,0,0" VerticalAlignment="Top" />
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
